{
  "name": "@sealsystems/eslint-config-es",
  "version": "3.3.11",
  "description": "ESLint executable and configuration for SEAL Systems",
  "author": {
    "name": "SEAL Systems AG",
    "email": "opensource@sealsystems.de",
    "url": "https://www.sealsystems.de"
  },
  "license": "MIT",
  "bin": {
    "bot": "bin/bot",
    "lint": "bin/lint",
    "lint-fix": "bin/lint-fix"
  },
  "main": "lib/index.js",
  "scripts": {
    "bot": "node bin/bot",
    "licenses": "license-checker --production --csv",
    "lint:fix": "node bin/lint-fix",
    "lint": "node bin/lint",
    "release": "release",
    "test": "jest",
    "test:coverage": "test-coverage",
    "test:report": "test-report"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sealsystems/node-eslint-config-es.git"
  },
  "dependencies": {
    "depcheck": "0.7.2",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-jest": "22.4.1",
    "eslint-plugin-mocha": "5.3.0",
    "eslint-plugin-prettier": "3.0.1",
    "prettier": "1.16.4",
    "shelljs": "0.8.3"
  },
  "devDependencies": {
    "@sealsystems/semantic-release": "4.0.8",
    "@sealsystems/jest": "2.0.1",
    "license-checker": "25.0.1"
  },
  "release": {
    "extends": "@sealsystems/semantic-release/node-module"
  },
  "publishConfig": {
    "access": "public"
  }
}
